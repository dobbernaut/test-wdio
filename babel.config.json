{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@config": "./config",
          "@constant": "./constants",
          "@service": "./services",
          "@page": "./pages"
        }
      }
    ]
  ]
}
